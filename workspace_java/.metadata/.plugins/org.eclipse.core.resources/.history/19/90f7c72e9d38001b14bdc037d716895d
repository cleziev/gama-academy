package Conta;

public class Banco {
	public static void main(String[] args) {
		
		Dados c1 = new Dados();
		
				
		c1.setConta(122445);
		c1.setNome("Jose");
		c1.setCpf("123.456.789-10");
		
		System.out.println(c1);
		
		ContaEspecial c2 = new ContaEspecial();
		
		c2.setNome("joao");
		c2.setConta(87566);
		c2.setCpf("123.345.456-10");
		c2.setLimite(300);
		
		System.out.println(c2);
	
}
}
