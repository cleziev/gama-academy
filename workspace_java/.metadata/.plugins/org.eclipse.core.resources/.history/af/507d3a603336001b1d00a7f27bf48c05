
public class Banco {
	public static void main(String[] args) {
		
		DadosConta c1, c2;
		
		c1 = new DadosConta();
		c2 = new DadosConta();
		
		c1.setNumeroDaConta(987654);
		c1.setTitular("Jose Capiroto");
		c1.setCPF("250.000.345-20");
		c1.getSaldo(0.0);
		
		c1.setNumeroDaConta(987654);
		c1.setTitular("Jose Capiroto");
		c1.setCPF("250.000.345-20");
		c1.getSaldo(0.0);

	
		
		System.out.println(c1.getSaldo());
		System.out.println(c2.getSaldo());
		
	}	

}
